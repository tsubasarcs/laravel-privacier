{
    "name": "tsubasarcs/privacier",
    "description": "Taiwan privacy policy record.",
    "keywords": ["laravel", "privacy"],
    "license": "MIT",
    "homepage": "https://github.com/tsubasarcs/privacier",
    "authors": [
        {
            "name": "tsubasarcs",
            "email": "tsubasarcs@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/laravel": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Tsubasarcs\\Privacier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsubasarcs\\Privacier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Tsubasarcs\\Privacier\\Providers\\PrivacyServiceProvider"
            ],
            "aliases": {
                "Privacier": "Tsubasarcs\\Privacier\\Facades\\Privacier"
            }
        }
    },
    "minimum-stability": "dev"
}
